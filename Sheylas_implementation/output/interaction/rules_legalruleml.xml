<?xml version="1.0" encoding="UTF-8"?>
<lrml:LegalRuleML
  xmlns:lrml="http://docs.oasis-open.org/legalruleml/ns/v1.0/"
  xmlns:ruleml="http://ruleml.org/spec"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

  <lrml:Statements>
    <lrml:PrescriptiveStatement key="r1">
      <ruleml:Rule key="r1">
        <ruleml:if>
          <ruleml:Atom>
            <ruleml:Rel>interactsWithPeople</ruleml:Rel>
            <ruleml:Var>AIsystem</ruleml:Var>
            <ruleml:Data xsi:type="xs:boolean">true</ruleml:Data>
          </ruleml:Atom>
        </ruleml:if>
        <ruleml:then>
          <ruleml:And>
              <ruleml:Atom>
                <ruleml:Rel>task</ruleml:Rel>
                <ruleml:Var>AIprovider</ruleml:Var>
                <ruleml:Data xsi:type="xs:string">hasInformationObligation</ruleml:Data>
              </ruleml:Atom>
              <ruleml:Atom>
                <ruleml:Rel>task</ruleml:Rel>
                <ruleml:Var>BPMNtool</ruleml:Var>
                <ruleml:Data xsi:type="xs:string">printMessage:LIMITED_RISK</ruleml:Data>
              </ruleml:Atom>
          </ruleml:And>
        </ruleml:then>
      </ruleml:Rule>
    </lrml:PrescriptiveStatement>

    <lrml:PrescriptiveStatement key="r2">
      <ruleml:Rule key="r2">
        <ruleml:if>
          <ruleml:Atom>
            <ruleml:Rel>interactsWithPeople</ruleml:Rel>
            <ruleml:Var>AIsystem</ruleml:Var>
            <ruleml:Data xsi:type="xs:boolean">false</ruleml:Data>
          </ruleml:Atom>
        </ruleml:if>
        <ruleml:then>
          <ruleml:Atom>
            <ruleml:Rel>task</ruleml:Rel>
            <ruleml:Var>AIprovider</ruleml:Var>
            <ruleml:Data xsi:type="xs:string">FollowVoluntaryCodeOfConduct</ruleml:Data>
          </ruleml:Atom>
        </ruleml:then>
      </ruleml:Rule>
    </lrml:PrescriptiveStatement>

    <lrml:PrescriptiveStatement key="r3">
      <ruleml:Rule key="r3">
        <ruleml:if>
          <ruleml:And>
              <ruleml:Atom>
                <ruleml:Rel>generatesSyntethicContent</ruleml:Rel>
                <ruleml:Var>AIsystem</ruleml:Var>
                <ruleml:Data xsi:type="xs:boolean">true</ruleml:Data>
              </ruleml:Atom>
              <ruleml:Atom>
                <ruleml:Rel>onlyEditsSyntheticContent</ruleml:Rel>
                <ruleml:Var>AIsystem</ruleml:Var>
                <ruleml:Data xsi:type="xs:boolean">true</ruleml:Data>
              </ruleml:Atom>
          </ruleml:And>
        </ruleml:if>
        <ruleml:then>
          <ruleml:And>
              <ruleml:Atom>
                <ruleml:Rel>task</ruleml:Rel>
                <ruleml:Var>AIprovider</ruleml:Var>
                <ruleml:Data xsi:type="xs:string">hasMarkingObligation</ruleml:Data>
              </ruleml:Atom>
              <ruleml:Atom>
                <ruleml:Rel>task</ruleml:Rel>
                <ruleml:Var>AIprovider</ruleml:Var>
                <ruleml:Data xsi:type="xs:string">FollowVoluntaryCodeOfConduct</ruleml:Data>
              </ruleml:Atom>
          </ruleml:And>
        </ruleml:then>
      </ruleml:Rule>
    </lrml:PrescriptiveStatement>

    <lrml:PrescriptiveStatement key="r4">
      <ruleml:Rule key="r4">
        <ruleml:if>
          <ruleml:And>
              <ruleml:Atom>
                <ruleml:Rel>generatesSyntethicContent</ruleml:Rel>
                <ruleml:Var>AIsystem</ruleml:Var>
                <ruleml:Data xsi:type="xs:boolean">true</ruleml:Data>
              </ruleml:Atom>
              <ruleml:Atom>
                <ruleml:Rel>onlyEditsSyntheticContent</ruleml:Rel>
                <ruleml:Var>AIsystem</ruleml:Var>
                <ruleml:Data xsi:type="xs:boolean">false</ruleml:Data>
              </ruleml:Atom>
          </ruleml:And>
        </ruleml:if>
        <ruleml:then>
          <ruleml:And>
              <ruleml:Atom>
                <ruleml:Rel>task</ruleml:Rel>
                <ruleml:Var>AIprovider</ruleml:Var>
                <ruleml:Data xsi:type="xs:string">hasMarkingObligation</ruleml:Data>
              </ruleml:Atom>
              <ruleml:Atom>
                <ruleml:Rel>task</ruleml:Rel>
                <ruleml:Var>BPMNtool</ruleml:Var>
                <ruleml:Data xsi:type="xs:string">printMessage:LIMITED_RISK</ruleml:Data>
              </ruleml:Atom>
          </ruleml:And>
        </ruleml:then>
      </ruleml:Rule>
    </lrml:PrescriptiveStatement>

    <lrml:PrescriptiveStatement key="r5">
      <ruleml:Rule key="r5">
        <ruleml:if>
          <ruleml:Atom>
            <ruleml:Rel>generatesSyntethicContent</ruleml:Rel>
            <ruleml:Var>AIsystem</ruleml:Var>
            <ruleml:Data xsi:type="xs:boolean">false</ruleml:Data>
          </ruleml:Atom>
        </ruleml:if>
        <ruleml:then>
          <ruleml:Atom>
            <ruleml:Rel>task</ruleml:Rel>
            <ruleml:Var>AIprovider</ruleml:Var>
            <ruleml:Data xsi:type="xs:string">FollowVoluntaryCodeOfConduct</ruleml:Data>
          </ruleml:Atom>
        </ruleml:then>
      </ruleml:Rule>
    </lrml:PrescriptiveStatement>

    <lrml:OverrideStatement>
      <lrml:Override over="#r1" under="#r2"/>
    </lrml:OverrideStatement>

    <lrml:OverrideStatement>
      <lrml:Override over="#r2" under="#r3"/>
    </lrml:OverrideStatement>

    <lrml:OverrideStatement>
      <lrml:Override over="#r3" under="#r4"/>
    </lrml:OverrideStatement>

    <lrml:OverrideStatement>
      <lrml:Override over="#r4" under="#r5"/>
    </lrml:OverrideStatement>
  </lrml:Statements>

</lrml:LegalRuleML>
