              <ruleml:Atom>
                <ruleml:Rel>itworkonalargeamountofdata</ruleml:Rel>
                <ruleml:Var>Does</ruleml:Var>
                <ruleml:Data xsi:type="xs:boolean">true</ruleml:Data>
              </ruleml:Atom>
              <ruleml:Atom>
                <ruleml:Rel>itopen-accesandfreelicense</ruleml:Rel>
                <ruleml:Var>Is</ruleml:Var>
                <ruleml:Data xsi:type="xs:boolean">true</ruleml:Data>
              </ruleml:Atom>
              <ruleml:Atom>
                <ruleml:Rel>substancialinfo</ruleml:Rel>
                <ruleml:Var>Gives</ruleml:Var>
                <ruleml:Data xsi:type="xs:boolean">false</ruleml:Data>
              </ruleml:Atom>
              <ruleml:Atom>
                <ruleml:Rel>morethan10(25)</ruleml:Rel>
                <ruleml:Var>FLOP</ruleml:Var>
                <ruleml:Data xsi:type="xs:boolean">true</ruleml:Data>
              </ruleml:Atom>
          </ruleml:And>
        </ruleml:if>
        <ruleml:then>
          <ruleml:And>
              <ruleml:Atom>
                <ruleml:Rel>task</ruleml:Rel>
                <ruleml:Var>Print</ruleml:Var>
                <ruleml:Data xsi:type="xs:string">message:GPAI</ruleml:Data>
              </ruleml:Atom>
              <ruleml:Atom>
                <ruleml:Rel>task</ruleml:Rel>
                <ruleml:Var>GPAI</ruleml:Var>
                <ruleml:Data xsi:type="xs:string">openandfree</ruleml:Data>
              </ruleml:Atom>
              <ruleml:Atom>
                <ruleml:Rel>task</ruleml:Rel>
                <ruleml:Var>GPAI</ruleml:Var>
                <ruleml:Data xsi:type="xs:string">oblwithrisk</ruleml:Data>
